{
  "name": "angular-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build:dev": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker:build": "docker build -t angular-starter-prod . && docker run -p 8080:80 --name angular-starter angular-starter-prod",
    "docker:compose:build": "docker-compose up -d --build",
    "docker:compose:run": "docker-compose run cypress ./node_modules/.bin/cypress run --spec 'cypress/integration/starter/**'",
    "docker:compose:down": "docker-compose down",
    "format:all": "npm run format:prettier && npm run format:html",
    "format:prettier": "node_modules/.bin/prettier --config .prettierrc \"src/**/*.{ts,css,js}\" --write",
    "format:html": "node bin/glob-ls.js 'src/**/*.html' 'build/**/*.html.ejs' | xargs node_modules/.bin/html-beautify -r",
    "docs:compodoc": "npx compodoc -p src/tsconfig.app.json",
    "docs:compodoc:serve": "npx compodoc -p src/tsconfig.app.json -s",
    "docs:compodoc:serve:watch": "npx compodoc -p src/tsconfig.app.json -s -w",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --spec 'cypress/integration/starter/**'",
    "cypress:run-record": "cypress run --record --key <MyKey> --spec 'cypress/integration/starter/**'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.14",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.14",
    "@angular/compiler": "^7.2.14",
    "@angular/core": "^7.2.14",
    "@angular/forms": "^7.2.14",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.14",
    "@angular/platform-browser-dynamic": "^7.2.14",
    "@angular/router": "^7.2.14",
    "@ngxs/store": "^3.4.3",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "^7.2.14",
    "@angular/language-service": "^7.2.14",
    "@compodoc/compodoc": "^1.1.9",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "glob": "^7.1.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "js-beautify": "^1.10.0",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "^1.17.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
